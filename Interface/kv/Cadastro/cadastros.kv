<CadastroScreen>:
    size_hint: 0.1,0.1
    name: "cadastro"
    Label:
        text: 'Cadastros'
        font_size: 0.6*self.height
        pos_hint: {'top':9,'right':5.5}
        size_hint: 1, 1
    Button:
        font_size: 0.4*self.height
        text:'Pessoa'
        pos_hint: {'top':7, 'right': 5}
        on_release: app.root.current = "pessoa"
        size_hint: 2, .8
    Button:
        font_size: 0.4*self.height
        text:'Investimentos Fixos'
        pos_hint: {'top':7, 'right': 7}
        on_release: app.root.current = "investimento"
        size_hint: 2, .8
    Button:
        font_size: 0.4*self.height
        text:'Materia Prima'
        pos_hint: {'top':6, 'right': 5}
        on_release: app.root.current = "matprima"
        size_hint: 2, .8
    Button:
        font_size: 0.4*self.height
        text:'Estimativa de Vendas'
        pos_hint: {'top':6, 'right': 7}
        on_release: app.root.current = "estimativa"
        size_hint: 2, .8
    Button:
        font_size: 0.4*self.height
        text:'Custos Fixos Mensais'
        pos_hint: {'top':5, 'right': 7}
        on_release: app.root.current = "custofixo"
        size_hint: 2, .8
    Button:
        font_size: 0.4*self.height
        text:'Tributos'
        pos_hint: {'top':5, 'right': 5}
        on_release: app.root.current = "tributo"
        size_hint: 2, .8
    Button:
        font_size: 0.4*self.height
        text:'Custo com Vendas'
        pos_hint: {'top':4, 'right': 5}
        on_release: app.root.current = "custovendas"
        size_hint: 2, .8
    Voltar:
        on_release: app.root.current = "menu"

