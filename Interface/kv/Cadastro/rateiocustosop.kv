<RateioCustosOpScreen>:
    size_hint: 0.1,0.1
    id: rateiocustosop
    name: "rateiocustosop"

    Label:
        text: 'Rateio dos Custos Operacionais'
        font_size: 0.6*self.height
        pos_hint: {'top':9,'right':5.5}
        size_hint: 1, 1
    Label:
        text: 'Produto'
        font_size: 0.4*self.height
        pos_hint: {'top':8,'right':2}
        size_hint: 1, 1
    Label:
        text: '% de rateio'
        font_size: 0.4*self.height
        pos_hint: {'top':8,'right':4}
        size_hint: 1, 1
    SendButton:
        on_release: rateiocustosop.envia()
    Voltar:
        on_release: app.root.current = "cadastro"