<RateioCustosFixosScreen>:
    id: rateiocustosfixos
    name: "rateiocustosfixos"
    scrl: scrl
    ScrollView:
        BoxLayout:
            size_hint_y:None
            height: root.height * 1.5
            id: scrl
            orientation: 'vertical'
            Label:
                text: 'Rateio dos Custos Fixos'
                font_size: 0.6*self.height

            Label:
                text: 'Produto'
                font_size: 0.4*self.height
            Label:
                text: '% de rateio'
                font_size: 0.4*self.height
    SendButton:
        on_release: rateiocustosfixos.envia()
    Voltar:
        on_release: app.root.current = "cadastro"