Manager:
    StartScreen:
    MenuScreen:
    Cadastro:
    Relatorio:
    Alterar:
    PessoaScreen:
    Investimento:
    MateriaPrimaScreen:
    EstimativaScreen:

<StartScreen>:
    size_hint: 0.1, 0.1
    name: "start"
    Label:
        text: 'GIBATABLE'
        font_size: 0.6*self.height
        pos_hint: {'top':7,'right':5.5}

    StartButton:
        pos_hint: {'top':6,'right':6}
        size_hint: 2, .8
        on_release: app.root.current = "menu"


<MenuScreen>:
    size_hint: 0.1,0.1
    name: "menu"
    Label:
        text: 'MENU'
        font_size: 0.6*self.height
        pos_hint: {'top':9,'right':5.5}
        size_hint: 1, 1

    Button:
        font_size: 0.4*self.height
        text:'Cadastros'
        pos_hint: {'top':8, 'right': 4}
        on_release: app.root.current = "cadastro"
        size_hint: 2, .8

    Button:
        font_size: 0.4*self.height
        text:'Relatorios'
        pos_hint: {'top':8, 'right': 8}
        on_release: app.root.current = "relatorio"
        size_hint: 2, .8

    Button:
        font_size: 0.4*self.height
        text:'Alterar'
        pos_hint: {'top':7, 'right': 6}
        on_release: app.root.current = "alterar"
        size_hint: 2, .8

    Voltar:
        on_release: app.root.current = "start"

<Alterar>:
    size_hint: 0.1,0.1
    name: "alterar"
    Label:
        text: 'Selecione de qual sessao quer alterar os dados:'
        font_size: 0.6*self.height
        pos_hint: {'top':9,'right':5.5}
        size_hint: 1, 1
    Voltar:
        on_release: app.root.current = "menu"